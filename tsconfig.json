{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "allowJs": true,
    "strictPropertyInitialization": false,
    "strict": false,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "diagnostics": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@app/*": ["./src/app/*"]
    }
  },
  "angularCompilerOptions": {
      "preserveWhitespaces": true
  },
}
